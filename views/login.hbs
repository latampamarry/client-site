    <link rel="stylesheet"
        href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/themes/smoothness/jquery-ui.css" />
    <link rel="stylesheet" href="/s/c/app.css" type="text/css" />
    <!-- <link rel="stylesheet" href="s/j/datepicker/foundation-datepicker.css" /> -->

    <meta name="description" content="Skip the games. Get satisfaction. Meet top-rated escorts and clients." />
    <meta name="keywords" content=" adult dating, escorts, review board" />
    <meta name="rating" content="RTA-5042-1996-1400-1577-RTA" />
    <title>Log in to your Skipthegames.com account</title>
</head>

<body>
    <noscript>
        <p>To make the most out of your visit to Skipthegames.com, please <a href="https://www.enable-javascript.com/"
                target="_blank">enable Javascript</a></p>
    </noscript>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script>
    <script src="/s/j/202.js"></script>



    <style>
        #login-marker {
            display: none !important
        }

        .row {
            margin: 0;
            padding: 0;
        }

        .padding-top{
            padding-top: .9375em;
            padding-bottom: .9375em;
        }
    </style>

    <script>
        function onSubmit(token) {
            document.getElementById("form_createad_existing_account_login").submit();
        }
        $(function () {
            $('#input_account_email').focus(function () {
                this.select();
            });
            if ($('#input_account_email').val()) {
                $('#input_account_password').focus();
            } else {
                $('#input_account_email').focus();
            }
        });
    </script>
    <div class="row">
        <div class="small-16 medium-7 columns login-form padding-top">

            <form action="/auth/login" method="POST" id="form_createad_existing_account_login" data-abide>
                <!-- <input type="hidden" name="_xsrf" value="e3919a2747754cf9b732b82027a7b288" /> -->
                <p><input type="email" id="input_account_email" name="input_account_email" placeholder="Your email"
                        required value="" /><small class="error">Please enter a valid email address</small></p>
                <p><input type="password" id="input_account_password" name="input_account_password"
                        placeholder="Password" required /><small class="error">Please enter your password</small></p>
                <input type="hidden" name="ref_id" value="{{refId}}" />
                <!-- replacing id="submit_createad_account_login"  with id="submit_credentials"-->
                <input type="submit" class="expand radius button" data-callback='submitCreds' id="submit_credentials"
                    value="Log in" />

            </form>
        </div>
    </div>


    <script>
        $(document).foundation(
            {
                offcanvas:
                {
                    close_on_click: false
                }
            });
        var doc = document.documentElement;
        doc.setAttribute('data-useragent', navigator.userAgent);

        //Opera does not do Accordion
        $('html[data-useragent*="Opera"] .accordion dd div.content').css('display', 'block');

    </script>
</body>

</html>